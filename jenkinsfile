properties([
    [$class: 'GithubProjectProperty',
     displayName: '',
     projectUrlStr: 'https://github.com/saipradeep41/jenkinspipeline/'],
     pipelineTriggers([githubPush()])])

pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building.'
            }
        }
        stage('Test') {
            steps {
                echo 'musting.'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
